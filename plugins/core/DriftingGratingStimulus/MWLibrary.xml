<MWElements>
	<MWElement name="Drifting Grating Stimulus">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>stimulus[@type="drifting_grating"]</match_signature>
		
		<isa>Stimulus</isa>
		
		<description>
			A drifting grating stimulus
        </description>
		
		<!-- Payload -->
		<code>
			<stimulus type="drifting_grating" tag="New Drifting Grating"
			direction="0"
			starting_phase="0"
			spatial_frequency="1"
			speed="1"
			grating_type="sinusoid"
			mask="gaussian"
			inverted="NO"
			std_dev="1"
			mean="0"
			normalized="YES"
			x_size="1"
			y_size="1"
			x_position="0"
			y_position="0"
			rotation="0"
			alpha_multiplier="1"
			autoplay="0"/>
        </code>
    </MWElement>
</MWElements>
