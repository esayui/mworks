<MWElements>
  <MWElement name="Firmata Device">
    <match_signature>iodevice[@type="firmata"]</match_signature>
    
    <isa>IODevice</isa>

    <icon>smallIOFolder</icon>
    
    <description>
Interface to a microcontroller that supports the Firmata protocol
    </description>
    
    <code>
      <iodevice type="firmata"
                tag="Firmata Device"
                serial_port=""
                bluetooth_local_name=""
                data_interval=""
                />
    </code>
  </MWElement>
   
  <MWElement name="Firmata Analog Input Channel">
    <match_signature>iochannel[@type="firmata_analog_input"]</match_signature>
    
    <isa>IOChannel</isa>
    <allowed_parent>Firmata Device</allowed_parent>

    <icon>smallIOFolder</icon>
    
    <description>
Analog input channel on a Firmata device
    </description>
    
    <code>
      <iochannel type="firmata_analog_input"
                 tag="Firmata Analog Input Channel"
                 pin_number=""
                 value=""
             />
    </code>
  </MWElement>
   
