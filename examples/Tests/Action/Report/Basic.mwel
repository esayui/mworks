var greeting = ''
var subject = ''


protocol {
    //
    // Constant message
    //
    
    report ('Hello, World!')

    //
    // Non-constant messsage
    //

    greeting = 'Good morning'
    subject = 'Bob'

    report ('$greeting, ${subject}!')
    report ('$greeting, $(subject + "!")')
    report ('$(2 + 3) != $(2 * 3)')
}
